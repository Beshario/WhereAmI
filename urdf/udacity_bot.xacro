<?xml version='1.0'?>

<robot name="udacity_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">


	<link name='chassis'>
		<inertial>
		  <mass value="0.489"/>
		  <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
		  <inertia
			  ixx="0.1" ixy="0" ixz="0"
			  iyy="0.1" iyz="0"
			  izz="0.1"
		  />
		</inertial>

		<collision name='collision'>
		  <origin xyz="0 0 0" rpy=" 0 0 0"/> 
		  <geometry>
			<box size=".12 .14 .05"/>
		  </geometry>
		</collision>

		<visual name='chassis_visual'>
		  <origin xyz="0 0 0" rpy=" 0 0 0"/>
		  <geometry>
			<box size=".12 .14 .05"/>
		  </geometry>
		</visual>
	</link>

	<link name='rightb_wheel'>
		<visual>
			  <geometry>
				<cylinder length="0.025" radius="0.03"/>
			  </geometry>
		</visual>
		
		<inertial>
			  <mass value="0.033"/>
			  <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
			  <inertia
				  ixx="0.1" ixy="0" ixz="0"
				  iyy="0.1" iyz="0"
				  izz="0.1"
			  />
    	</inertial>
    
		<collision name='collision'>
			  <origin xyz="0 0 0" rpy=" 0 0 0"/> 
			  <geometry>
				<cylinder length="0.025" radius="0.03"/>
			  </geometry>
			</collision>
	</link>
	<link name='leftb_wheel'>
		<visual>
			  <geometry>
				<cylinder length="0.025" radius="0.03"/>
			  </geometry>
		</visual>
		<inertial>
			  <mass value="0.033"/>
			  <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
			  <inertia
				  ixx="0.1" ixy="0" ixz="0"
				  iyy="0.1" iyz="0"
				  izz="0.1"
			  />
    	</inertial>
		<collision name='collision'>
			  <origin xyz="0 0 0" rpy=" 0 0 0"/> 
			  <geometry>
				<cylinder length="0.025" radius="0.03"/>
			  </geometry>
			</collision>
	</link>

	<link name='rightf_wheel'>
		<visual>
			  <geometry>
				<cylinder length="0.025" radius="0.03"/>
			  </geometry>
		</visual>
		
		<inertial>
			  <mass value="0.033"/>
			  <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
			  <inertia
				  ixx="0.1" ixy="0" ixz="0"
				  iyy="0.1" iyz="0"
				  izz="0.1"
			  />
    		</inertial>
    
		<collision name='collision'>
			  <origin xyz="0 0 0" rpy=" 0 0 0"/> 
			  <geometry>
				<cylinder length="0.025" radius="0.03"/>
			  </geometry>
			</collision>
	</link>
	<link name='leftf_wheel'>
		<visual>
			  <geometry>
				<cylinder length="0.025" radius="0.03"/>
			  </geometry>
		</visual>
		<inertial>
			  <mass value="0.033"/>
			  <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
			  <inertia
				  ixx="0.1" ixy="0" ixz="0"
				  iyy="0.1" iyz="0"
				  izz="0.1"
			  />
    	</inertial>
		<collision name='collision'>
			  <origin xyz="0 0 0" rpy=" 0 0 0"/> 
			  <geometry>
				<cylinder length="0.025" radius="0.03"/>
			  </geometry>
			</collision>
	</link>

	<joint type="continuous" name="left__back_wheel_hinge">
		<origin xyz="0.06 -0.07 0" rpy="1.570796 0 0"/>
		<child link="leftb_wheel"/>
		<parent link="chassis"/>
		<axis xyz="0 1 0" rpy="0 0 0"/>
		<limit effort="10000" velocity="1000"/>
		<joint_properties damping="1.0" friction="1.0"/>
  	</joint>
  	
  	<joint type="continuous" name="right_back_wheel_hinge">
		<origin xyz="0.06 0.07 0" rpy="1.570796 0 0"/>
		<child link="rightb_wheel"/>
		<parent link="chassis"/>
		<axis xyz="0 1 0" rpy="0 0 0"/>
		<limit effort="10000" velocity="1000"/>
		<joint_properties damping="1.0" friction="1.0"/>
  	</joint>

	<joint type="continuous" name="left__front_wheel_hinge">
		<origin xyz="-0.06 -0.07 0" rpy="1.570796 0 0"/>
		<child link="leftf_wheel"/>
		<parent link="chassis"/>
		<axis xyz="0 1 0" rpy="0 0 0"/>
		<limit effort="10000" velocity="1000"/>
		<joint_properties damping="1.0" friction="1.0"/>
  	</joint>
  	
  	<joint type="continuous" name="right_front_wheel_hinge">
		<origin xyz="-0.06 0.07 0" rpy="1.570796 0 0"/>
		<child link="rightf_wheel"/>
		<parent link="chassis"/>
		<axis xyz="0 1 0" rpy="0 0 0"/>
		<limit effort="10000" velocity="1000"/>
		<joint_properties damping="1.0" friction="1.0"/>
  	</joint>
</robot>
